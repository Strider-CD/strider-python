{
  "name": "strider-python",
  "version": "0.2.3",
  "description": "Python/Pyramid/Django support for Strider Continuous Deployment",
  "scripts": {
    "test": "jshint && mocha -R tap"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-python.git"
  },
  "keywords": [
    "strider",
    "python"
  ],
  "strider": {
    "type": "job",
    "id": "python",
    "title": "Python",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": true
  },
  "author": "Niall O'Higgins",
  "license": "BSD",
  "devDependencies": {
    "chai": "~3.5.0",
    "jshint": "~2.9.4",
    "mocha": "~3.2.0"
  }
}
